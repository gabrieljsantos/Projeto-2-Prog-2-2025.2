{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Executar com Debugger",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/escola-de-musica-benicasa/bin/Debug/escola-de-musica-benicasa.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
